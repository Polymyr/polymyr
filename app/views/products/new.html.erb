<div class="form-wrapper product-form">
	
	<h2>Launch your product</h2>

	<hr>

	<%= simple_form_for @product do |f| %>

		<div class="form-inputs">
			<%= f.input :name, label: "Product name" %>
		</div>

		<div class="form-actions">
	    <%= f.button :submit, "Submit" %>
	  </div>
	<% end %>
</div>