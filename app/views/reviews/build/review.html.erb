<div class="form-wrapper">	
	<%= simple_form_for @review, url: wizard_path, method: :put do |f| %>

		<div class="form-inputs">
			<%= f.input :q16, label: "Question 16" %>
			<%= f.input :q17, label: "Question 17" %>
			<%= f.input :q18, label: "Question 18" %>
			<%= f.input :q19, label: "Question 19" %>
			<%= f.input :q20, label: "Question 20" %>
		</div>

		<div class="form-actions">
	    <%= f.button :submit, "Submit" %>
	  </div>
	<% end %>
</div>