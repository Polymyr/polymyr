<%= simple_form_for @review, url: wizard_path, method: :put do |f| %>

	<div class="form-inputs">
		<%= f.input :q1, label: "Question 1" %>
		<%= f.input :q2, label: "Question 2" %>
		<%= f.input :q3, label: "Question 3" %>
		<%= f.input :q4, label: "Question 4" %>
		<%= f.input :q5, label: "Question 5" %>
	</div>

	<div class="form-actions">
    <%= f.button :submit, "Submit" %>
  </div>
<% end %>